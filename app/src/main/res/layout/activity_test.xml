<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable
            name="test"
            type="com.humaxdigital.automotive.v2xpoc.presentation.v2x.TestViewModel" />
    </data>
    <LinearLayout
        android:layout_width="1920px"
        android:layout_height="630px"
        android:orientation="horizontal"
        android:weightSum="4"
        tools:context=".presentation.v2x.TestActivity">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:weightSum="4"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="3"
                android:orientation="vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="warningType"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="warningDirection"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="warningSeverity"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="warningRange"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="warningIconId"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="warningAudioId"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="warningTextId"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="warningPushed"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/warningType"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@{test.warningType.toString()}"/>
                <TextView
                    android:id="@+id/warningDirection"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@{test.warningDirection.toString()}"/>
                <TextView
                    android:id="@+id/warningSeverity"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@{test.warningSeverity.toString()}"/>
                <TextView
                    android:id="@+id/warningRange"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@{test.warningRange.toString()}"/>
                <TextView
                    android:id="@+id/warningIconId"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@{test.warningIconId.toString()}"/>
                <TextView
                    android:id="@+id/warningAudioId"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@{test.warningAudioId.toString()}"/>
                <TextView
                    android:id="@+id/warningTextId"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@{test.warningTextId.toString()}"/>
                <TextView
                    android:id="@+id/warningPushed"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@{test.warningPushed.toString()}"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:weightSum="4"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="3"
                android:orientation="vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="informType"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="informDirection"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="informSeverity"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="informRange"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="informIconId"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="informAudioId"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="informTextId"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="informPushed"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/informType"
