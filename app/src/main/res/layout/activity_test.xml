                <TextView
                    android:id="@+id/informPushed"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@{test.informPushed.toString()}"/>
                <TextView
                    android:id="@+id/informIconId"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@{test.informIconId.toString()}"/>
                <TextView
                    android:id="@+id/informAudioId"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@{test.informAudioId.toString()}"/>
                <TextView
                    android:id="@+id/informTextId"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@{test.informTextId.toString()}"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
